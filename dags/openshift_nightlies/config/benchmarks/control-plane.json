{
    "benchmarks": [
        {
            "name": "node-density",
            "workload": "kube-burner",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "node-density-cni",
                "PODS_PER_NODE": "45",
                "NODE_COUNT": "21",
                "JOB_TIMEOUT": "18000",
                "QPS": "20",
                "BURST": "20",
                "STEP_SIZE": "30s",
                "LOG_LEVEL": "info",
                "LOG_STREAMING": "true",
                "CLEANUP_WHEN_FINISH": "true",
                "CLEANUP": "true",
                "MUST_GATHER_EACH_TASK": "true",
                "NODE_SELECTOR": "{node-role.kubernetes.io/workload: }",
                "METRICS_PROFILE": "metrics-profiles/metrics-ovn.yaml",
                "GSHEET_KEY_LOCATION": "/root/.secrets/gsheet_key.json",
                "EMAIL_ID_FOR_RESULTS_SHEET": "msheth@redhat.com",
                "COMPARISON_CONFIG": "clusterVersion.json podLatency.json podCPU-avg.json podCPU-max.json podMemory-avg.json podMemory-max.json"
            }
        },
        {
            "name": "node-density-cni",
            "workload": "kube-burner",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "node-density-cni-networkpolicy",
                "PODS_PER_NODE": "45",
                "NODE_COUNT": "21",
                "JOB_TIMEOUT": "18000",
                "QPS": "20",
                "BURST": "20",
                "STEP_SIZE": "30s",
                "LOG_LEVEL": "info",
                "LOG_STREAMING": "true",
                "CLEANUP_WHEN_FINISH": "true",
                "CLEANUP": "true",
                "MUST_GATHER_EACH_TASK": "true",
                "NODE_SELECTOR": "{node-role.kubernetes.io/workload: }",
                "METRICS_PROFILE": "metrics-profiles/metrics-ovn.yaml",
                "GSHEET_KEY_LOCATION": "/root/.secrets/gsheet_key.json",
                "EMAIL_ID_FOR_RESULTS_SHEET": "msheth@redhat.com",
                "COMPARISON_CONFIG": "clusterVersion.json podLatency.json podCPU-avg.json podCPU-max.json podMemory-avg.json podMemory-max.json"
            }
        },
        {
            "name": "node-density-cni-networkpolicy",
            "workload": "kube-burner",
            "trigger_rule": "all_done",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "node-density",
                "PODS_PER_NODE": "245",
                "NODE_COUNT": "21",
                "JOB_TIMEOUT": "18000",
                "QPS": "20",
                "BURST": "20",
                "STEP_SIZE": "30s",
                "LOG_LEVEL": "info",
                "LOG_STREAMING": "true",
                "CLEANUP_WHEN_FINISH": "true",
                "CLEANUP": "true",
                "MUST_GATHER_EACH_TASK": "true",
                "NODE_SELECTOR": "{node-role.kubernetes.io/workload: }",
                "METRICS_PROFILE": "metrics-profiles/metrics-ovn.yaml",
                "GSHEET_KEY_LOCATION": "/root/.secrets/gsheet_key.json",
                "EMAIL_ID_FOR_RESULTS_SHEET": "msheth@redhat.com",
                "COMPARISON_CONFIG": "clusterVersion.json podLatency.json podCPU-avg.json podCPU-max.json podMemory-avg.json podMemory-max.json"
            }
        },
        {
            "name": "cluster-density",
            "workload": "kube-burner",
            "trigger_rule": "all_done",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "cluster-density",
                "JOB_ITERATIONS": "500",
                "JOB_TIMEOUT": "18000",
                "STEP_SIZE": "30s",
                "QPS": "20",
                "BURST": "20",
                "LOG_LEVEL": "info",
                "LOG_STREAMING": "true",
                "CLEANUP_WHEN_FINISH": "true",
                "CLEANUP": "true",
                "MUST_GATHER_EACH_TASK": "true",
                "NODE_SELECTOR": "{node-role.kubernetes.io/workload: }",
                "METRICS_PROFILE": "metrics-profiles/metrics-ovn.yaml",
                "GSHEET_KEY_LOCATION": "/root/.secrets/gsheet_key.json",
                "EMAIL_ID_FOR_RESULTS_SHEET": "msheth@redhat.com",
                "COMPARISON_CONFIG": "clusterVersion.json podLatency.json podCPU-avg.json podCPU-max.json podMemory-avg.json podMemory-max.json"
            }
        }
    ]
}
