{
    "benchmarks": [
        {
            "name": "host-network",
            "workload": "network-perf",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "hostnet",
                "PAIRS": "1",
		"GEN_CSV": "true",
		"EMAIL_ID_FOR_RESULTS_SHEET": "msheth@redhat.com",
		"COMPARISON_CONFIG": "uperf-touchstone.json",
		"TOLERANCY_RULES_CFG": "uperf-tolerancy-rules.yaml",
		"GSHEET_KEY_LOCATION": "/tmp/key.json",
		"COMPARISON_RC": "0"
            }
        },
        {
            "name": "pod-network",
            "workload": "network-perf",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "pod2pod",
                "GEN_CSV": "true",
                "EMAIL_ID_FOR_RESULTS_SHEET": "msheth@redhat.com",
                "COMPARISON_CONFIG": "uperf-touchstone.json",
                "TOLERANCY_RULES_CFG": "uperf-tolerancy-rules.yaml",
                "GSHEET_KEY_LOCATION": "/tmp/key.json",
                "COMPARISON_RC": "0"
            }
        },
        {
            "name": "serviceip-network",
            "workload": "network-perf",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "pod2svc",
                "GEN_CSV": "true",
                "EMAIL_ID_FOR_RESULTS_SHEET": "msheth@redhat.com",
                "COMPARISON_CONFIG": "uperf-touchstone.json",
                "TOLERANCY_RULES_CFG": "uperf-tolerancy-rules.yaml",
                "GSHEET_KEY_LOCATION": "/tmp/key.json",
                "COMPARISON_RC": "0"
            }
        },
        {
            "name": "scale-27",
            "workload": "scale-perf",
            "command": "./run_scale_fromgit.sh",
            "env": {
                "SCALE": "27",
                "METADATA_COLLECTION": "true",
                "WORKLOAD_NODE_ROLE": "workload"
            }
        },
        {
            "name": "router",
            "workload": "router-perf-v2",
            "command": "./ingress-performance.sh",
            "env": {
                "LARGE_SCALE_THRESHOLD": "19",
                "ENGINE": "local",
                "LARGE_SCALE_ROUTES": "500",
                "COMPARE": "false",
                "EMAIL_ID_FOR_RESULTS_SHEET": "msheth@redhat.com",
                "GSHEET_KEY_LOCATION": "/tmp/key.json"
            }
        }
    ]
}
