{
    "benchmarks": [
        {
            "name": "scale-20",
            "workload": "scale-perf",
            "command": "./run_scale_fromgit.sh",
            "env": {
                "SCALE": "20",
                "METADATA_COLLECTION": "true",
                "WORKLOAD_NODE_ROLE": "workload"
            }
        },
        {
            "name": "networkpolicy-case2",
            "workload": "kube-burner",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "networkpolicy-case2",
                "JOB_TIMEOUT": "18000",
                "QPS": "20",
                "BURST": "20",
                "STEP_SIZE": "30s",
                "LOG_LEVEL": "info",
                "CLEANUP_WHEN_FINISH": "true",
                "CLEANUP": "true",
                "MUST_GATHER_EACH_TASK": "true",
                "NODE_SELECTOR": "{node-role.kubernetes.io/workload: }",
                "METRICS_PROFILE": "metrics-profiles/metrics-ovn.yaml",
                "GSHEET_KEY_LOCATION": "/root/.secrets/gsheet_key.json",
                "EMAIL_ID_FOR_RESULTS_SHEET": "msheth@redhat.com",
                "COMPARISON_CONFIG": "clusterVersion.json podLatency.json podCPU-avg.json podCPU-max.json podMemory-avg.json podMemory-max.json",
                "GEN_CSV": "true"
            }
        },
        {
            "name": "networkpolicy-case3",
            "workload": "kube-burner",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "networkpolicy-case3",
                "JOB_TIMEOUT": "18000",
                "QPS": "20",
                "BURST": "20",
                "STEP_SIZE": "30s",
                "LOG_LEVEL": "info",
                "CLEANUP_WHEN_FINISH": "true",
                "CLEANUP": "true",
                "MUST_GATHER_EACH_TASK": "true",
                "NODE_SELECTOR": "{node-role.kubernetes.io/workload: }",
                "METRICS_PROFILE": "metrics-profiles/metrics-ovn.yaml",
                "GSHEET_KEY_LOCATION": "/root/.secrets/gsheet_key.json",
                "EMAIL_ID_FOR_RESULTS_SHEET": "msheth@redhat.com",
                "COMPARISON_CONFIG": "clusterVersion.json podLatency.json podCPU-avg.json podCPU-max.json podMemory-avg.json podMemory-max.json",
                "GEN_CSV": "true"
            }
        }
    ]
}
