{
    "benchmarks": [
        {
            "name": "scale-27",
            "workload": "scale-perf",
            "command": "./run_scale_fromgit.sh",
            "env": {
                "SCALE": "27",
                "METADATA_COLLECTION": "false",
                "WORKLOAD_NODE_ROLE": "workload"
            }
        },
        {
            "name": "load-cluster-preupgrade",
            "workload": "kube-burner",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "cluster-density",
                "JOB_ITERATIONS": "500",
                "JOB_TIMEOUT": "18000",
                "STEP_SIZE": "2m",
                "QPS": "20",
                "BURST": "20",
                "LOG_LEVEL": "info",
                "CLEANUP_WHEN_FINISH": "false",
                "CLEANUP": "true",
                "MUST_GATHER_EACH_TASK": "true",
                "CLEANUP_TIMEOUT": "2h",
                "NODE_SELECTOR": "{node-role.kubernetes.io/workload: }",
                "METRICS_PROFILE": "metrics-profiles/metrics-aggregated.yaml",
                "ERROR_ON_VERIFY": "false"
            }
        },
        {
            "name": "upgrades",
            "workload": "upgrade-perf",
            "command": "./run_upgrade_fromgit.sh",
            "env": {
                "LATEST": "true",
                "CHANNEL": "nightlies",
                "TIMEOUT": "400",
                "POLL_INTERVAL": "10"
            }
        }
   ]
}
